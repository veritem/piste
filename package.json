{
	"name": "piste",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postinstall": "prisma generate",
		"postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
		"vercel-build": "prisma generate && prisma migrate deploy && npm run build",
		"ts-node": "ts-node --compiler-options '{\"module\": \"CommonJS\"}'",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@faker-js/faker": "^6.0.0",
		"@iconify/json": "^2.1.21",
		"@supabase/supabase-js": "^1.33.0",
		"@sveltejs/kit": "1.0.0-next.303",
		"@testing-library/svelte": "^3.1.0",
		"@types/cookie": "^0.4.1",
		"@types/node": "^17.0.23",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"@unocss/core": "^0.30.6",
		"@unocss/preset-attributify": "^0.30.6",
		"@unocss/preset-uno": "^0.30.6",
		"@unocss/preset-web-fonts": "^0.30.6",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"jsdom": "^19.0.0",
		"postcss-load-config": "^3.1.3",
		"prettier": "~2.6.1",
		"prettier-plugin-svelte": "^2.6.0",
		"prisma": "^3.11.1",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.6",
		"svelte-preprocess": "^4.10.4",
		"ts-node": "^10.7.0",
		"tslib": "^2.3.1",
		"typescript": "^4.6.3",
		"unocss": "^0.30.6",
		"vite": "^2.8.6",
		"vitest": "^0.7.12"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^3.11.1",
		"@sveltejs/adapter-vercel": "^1.0.0-next.47",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.40",
		"@unocss/preset-icons": "^0.30.6",
		"@unocss/reset": "^0.30.6",
		"cookie": "^0.4.2",
		"global": "^4.4.0",
		"svelte-select": "^4.4.7",
		"svelte-simple-modal": "^1.3.1"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	}
}
