{
	"name": "piste",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postinstall": "prisma generate",
		"postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
		"vercel-build": "prisma generate && prisma migrate deploy && npm run build",
		"ts-node": "ts-node --compiler-options '{\"module\": \"CommonJS\"}'"
	},
	"devDependencies": {
		"@supabase/supabase-js": "^1.28.2",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.1",
		"@types/faker": "^5.5.9",
		"@types/node": "^16.11.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"autoprefixer": "^10.4.0",
		"cssnano": "^5.0.12",
		"eslint": "^8.3.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"faker": "^5.5.3",
		"postcss": "^8.4.4",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.5.0",
		"prettier-plugin-svelte": "^2.5.0",
		"prisma": "^3.6.0",
		"svelte": "^3.44.2",
		"svelte-check": "^2.2.10",
		"svelte-preprocess": "^4.9.8",
		"tailwindcss": "^2.2.19",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.2"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^3.6.0",
		"@sveltejs/adapter-vercel": "^1.0.0-next.31",
		"@tailwindcss/forms": "^0.4.0",
		"cookie": "^0.4.1",
		"global": "^4.4.0",
		"svelte-simple-modal": "^1.1.1"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	}
}
